{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dark (Visual Studio)",
	"editor.guides.bracketPairsHorizontal": false,
	"colors": {
		"checkbox.border": "#6B6B6B",
		"editor.background": "#1e1f21",
		"editor.foreground": "#bcbec3",
		"editor.inactiveSelectionBackground": "#3A3D41",
		"editorIndentGuide.background1": "#404040",
		"editorIndentGuide.activeBackground1": "#707070",
		"editor.selectionHighlightBackground": "#ADD6FF26",
		"list.dropBackground": "#383B3D",
		"activityBarBadge.background": "#007ACC",
		"sideBarTitle.foreground": "#BBBBBB",
		"input.placeholderForeground": "#A6A6A6",
		"menu.background": "#252526",
		"menu.foreground": "#CCCCCC",
		"menu.separatorBackground": "#454545",
		"menu.border": "#454545",
		"menu.selectionBackground": "#0078d4",
		"statusBarItem.remoteForeground": "#FFF",
		"statusBarItem.remoteBackground": "#16825D",
		"ports.iconRunningProcessForeground": "#369432",
		"sideBarSectionHeader.background": "#0000",
		"sideBarSectionHeader.border": "#ccc3",
		"tab.selectedBackground": "#222222",
		"tab.selectedForeground": "#ffffffa0",
		"tab.lastPinnedBorder": "#ccc3",
		"list.activeSelectionIconForeground": "#FFF",
		"terminal.inactiveSelectionBackground": "#3A3D41",
		"widget.border": "#303031",
		"actionBar.toggledBackground": "#383a49",

		"editorBracketHighlight.foreground1": "#bcbec3",
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown",
				"variable.legacy.builtin.python"
			],
			"settings": {
				"foreground": "#bcbec3",
				"fontStyle": ""
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#b5cea8",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#646695",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.less"
			],
			"settings": {
				"foreground": "#d7ba7d",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#9cdcfe",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"source.css entity.other.attribute-name.class",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.parent.less",
				"source.css entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#d7ba7d",
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#b5cea8",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#ce9178",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"scope": "punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#6A9955",
				"fontStyle": ""
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#6796e6",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#ce9178",
				"fontStyle": ""
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#808080",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#ce9178",
				"fontStyle": ""
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#b5cea8",
				"fontStyle": ""
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#9cdcfe",
				"fontStyle": ""
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#bcbec3",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"source.css variable",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#9cdcfe",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#bcbec3",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#b5cea8",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#9cdcfe",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b5cea8",
				"fontStyle": ""
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#bcbec3",
				"fontStyle": ""
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": ""
			}
		},
		{
			"name": "comments",
			"scope": "comment",
			"settings": {
				"foreground": "#6d6f76",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.namespace",
			"settings": {
				"foreground": "#b2be85",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword.control.go",
			"settings": {
				"foreground": "#aab778",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#b8773f",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant.language.null.go",
			"settings": {
				"foreground": "#b0733c",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.type.go",
			"settings": {
				"foreground": "#7baab8",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.function.support.go",
			"settings": {
				"foreground": "#a29476",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.function.go",
			"settings": {
				"foreground": "#639ade",
				"fontStyle": ""
			}
		},
		{
			"scope": "string.quoted.double.go",
			"settings": {
				"foreground": "#78a977",
				"fontStyle": ""
			}
		},
		{
			"scope": "punctuation.definition.string.begin.go",
			"settings": {
				"foreground": "#78a977",
				"fontStyle": ""
			}
		},
		{
			"scope": "punctuation.definition.string.end.go",
			"settings": {
				"foreground": "#78a977",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#55a8b6",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant.other.placeholder.go",
			"settings": {
				"foreground": "#c59071",
				"fontStyle": ""
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator": "#bcbec3",
		"stringLiteral": "#ce9178",
		"customLiteral": "#bcbec3",
		"numberLiteral": "#b5cea8",
		"keyword": "#c59071",
		"method": "#78b0ff",
		"function.defaultLibrary": "#78b0ff",
		"variable.readonly.defaultLibrary": "#b0733c",
		"variable.readonly": "#b67cb2"
	}
}
